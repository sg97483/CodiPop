{
  "name": "codipop",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "i": "react-native run-ios --simulator='iPhone 15'",
    "a": "react-native run-android",
    "ios": "react-native run-ios",
    "iosd": "react-native run-ios --device '박민규의 iPhone' --mode Debug",
    "iosr": "react-native run-ios --device '박민규의 iPhone' --mode Release",
    "start": "react-native start",
    "metclean": "lsof -ti :8081 | xargs kill -9 && npx react-native start --reset-cache",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prebuild": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "pod": "cd ios && pod install && cd ..",
    "apk": "cd android && ./gradlew assembleRelease",
    "aab": "cd android && ./gradlew bundleRelease",
    "postinstall": "patch-package",
    "regenerate-bootsplash": "react-native generate-bootsplash src/assets/images/codipop_logo.png --background=\"#FFFFFF\" --logo-width=150 --assets-output=src/assets/images"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.1.1",
    "@invertase/react-native-apple-authentication": "2.3.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-camera-roll/camera-roll": "^7.10.2",
    "@react-native-firebase/app": "22.0.0",
    "@react-native-firebase/auth": "22.0.0",
    "@react-native-firebase/firestore": "22.0.0",
    "@react-native-firebase/messaging": "22.0.0",
    "@react-native-firebase/storage": "22.0.0",
    "@react-native-google-signin/google-signin": "10.1.0",
    "@react-navigation/bottom-tabs": "6.5.20",
    "@react-navigation/native": "6.1.9",
    "@react-navigation/native-stack": "6.9.7",
    "i18next": "^25.5.0",
    "react": "18.2.0",
    "react-i18next": "^15.7.3",
    "react-native": "0.74.2",
    "react-native-bootsplash": "5.1.4",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-localize": "^3.5.2",
    "react-native-permissions": "4.1.5",
    "react-native-reanimated": "~3.9.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-toast-message": "^2.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.28.3",
    "@react-native/babel-preset": "0.74.84",
    "@react-native/eslint-config": "0.74.84",
    "@react-native/metro-config": "0.74.84",
    "@react-native/typescript-config": "0.74.84",
    "@tsconfig/react-native": "^3.0.6",
    "@types/react": "^19.1.12",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "patch-package": "^8.0.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  }
}
